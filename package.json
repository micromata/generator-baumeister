{
  "name": "generator-baumeister",
  "version": "1.0.1",
  "description": "Yeoman Generator for »Baumeister«. The aim of this project is to help you to build your things. From Bootstrap themes over static websites to single page applications.",
  "license": "MIT",
  "main": "app/index.js",
  "homepage": "https://github.com/micromata/generator-baumeister",
  "repository": "micromata/generator-baumeister",
  "author": {
    "name": "Michael Kühnel",
    "email": "m.kuehnel@micromata.de",
    "url": "http://www.micromata.de"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublish": "nsp check",
    "lint": "eslint . --fix",
    "pretest": "npm run lint",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "release": "standard-version",
    "release:patch": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/app/templates"
    ],
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "app/**/*.js",
      "!app/templates/**/*.js"
    ],
    "coverageReporters": [
      "lcov",
      "html",
      "text",
      "text-summary"
    ]
  },
  "files": [
    "app"
  ],
  "keywords": [
    "yeoman-generator",
    "bootstrap",
    "boilerplate",
    "skeleton",
    "scaffolding",
    "scaffold",
    "theme",
    "themes",
    "kickstart",
    "kickstarter",
    "micromata",
    "workflow",
    "build",
    "gulp",
    "static site generator",
    "baumeister",
    "handlebars",
    "single page application",
    "single page applications",
    "es6",
    "jest"
  ],
  "dependencies": {
    "chalk": "1.1.3",
    "global": "^4.3.2",
    "grunt-cli": "^1.2.0",
    "semver": "5.3.0",
    "superb": "1.3.0",
    "underscore.string": "3.3.4",
    "yeoman-generator": "1.1.1",
    "yosay": "2.0.0"
  },
  "devDependencies": {
    "coveralls": "^2.13.1",
    "escape-string-regexp": "1.0.5",
    "eslint": "^4.1.0",
    "eslint-config-xo": "^0.18.2",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4",
    "nsp": "^2.6.3",
    "should": "11.2.1",
    "standard-version": "^4.2.0",
    "yeoman-assert": "^3.0.0",
    "yeoman-test": "^1.6.0"
  }
}
